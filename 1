import re

# define the regular expression pattern
pattern = r'a[b]*'

# test the pattern with some strings
strings = ['ab', 'aab', 'abbbbb', 'ac', 'abc']
for s in strings:
    match = re.search(pattern, s)
    if match:
        print(f"'{s}' matches the pattern")
    else:
        print(f"'{s}' does not match the pattern")